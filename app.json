{
  "expo": {
    "name": "Dritchwear",
    "slug": "dritch-wear",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "dritchwear",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#000000",
      "translucent": true
    },
    "androidNavigationBar": {
      "visible": "sticky-immersive",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to upload logos.",
        "NSCameraUsageDescription": "This app needs camera access to allow you take product or logo photos."
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-system-ui",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "minSdkVersion": 24,
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResourcesInReleaseBuilds": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "99a85905-b370-4a58-b525-5de48d3b5568"
      },
      "EXPO_PUBLIC_PAYPAL_CLIENT_ID": "enabled"
    },
    "owner": "ladyprowess",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/99a85905-b370-4a58-b525-5de48d3b5568"
    },
    "android": {
      "package": "com.dritchwear.app",
      "versionCode": 6,
      "permissions": [
        "NOTIFICATIONS",
        "READ_MEDIA_IMAGES"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    }
  }
}
