{
  "expo": {
    "name": "Dritchwear",
    "slug": "dritch-wear",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "dritchwear",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "99a85905-b370-4a58-b525-5de48d3b5568"
      },
      "EXPO_PUBLIC_PAYPAL_CLIENT_ID": "enabled"
    },
    "owner": "ladyprowess",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/99a85905-b370-4a58-b525-5de48d3b5568"
    },
    "android": {
      "package": "com.dritchwear.app",
      "versionCode": 1,
      "permissions": ["NOTIFICATIONS"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    }
  }
}
